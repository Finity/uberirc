<uirc-settings
	version="0.0"
>	
	<defaults
		nick="Guest"
		user="uberirc"
	/>
	<server
		name="AfterNet"
		url ="irc.afternet.org"
		nick="Guest"
		user="uberirc"
		auto="connect,reconnect"
	>
		<channel
			name="#gamedev"
			auto="join,rejoin"
			shortcut="Ctrl+1"
		/>
		<channel
			name="#graphicsdev"
			auto="join,rejoin"
			shortcut="Ctrl+2"
		/>
	</server>
</uirc-settings>
